{
   "items":
   [
      {   "type":"block", 
          text:"{{ "module.I5" | trans }}", 
          "text_pos": "top", 
          "list":
          [
             {   "type": "buttonSelect", 
                 text:"", 
                 "isbig": true, 
                 "img": "48/i5.png", 
                 items:
                 [ 
                    { id:"menu_jobs", text: "{{ "Jobs" | trans }}", img: "job.png" },
                    {   type: "buttonSelect", 
                        text: "{{ "Jobs" | trans }}", 
                        img:  "job.png", 
                        items:
                        [ 
                            { id:"menu_jobs", text: "{{ "Active jobs" | trans }}" },
                            { id:"menu_submitted", text: "{{ "Submitted jobs" | trans }}" },
                            { id:"menu_scheduled", text: "{{ "Scheduled jobs" | trans }}" }                            
                        ]
                    },
                    { id:"menu_messages", text: "{{ "Messages" | trans }}", img: "message.png" }
                 ]
              },
              {     "type":"button",
                    id:"menu_console", 
                    tooltip: "{{ "Console" | trans }}", 
                    img: "console.png" }
          ]
      },      
     {   "type":"block", 
          "id": "current_filter",
          {% if (app.session.get('user_filter')|length) >0 %}
          {% set filter = app.session.get('user_filter') %}
          text:"{{ filter.name }}",
          {% else %}
          text:"{{ "Filter" | trans }}", 
          {% endif %}
          "text_pos": "bottom", 
          "list":
          [
             {   "type": "buttonSelect", 
                 text:"",
                 "id": "Filters",
                 "isbig": true, 
                 "img": "48/search.png", 
                 items:
                 [ 
     {% if (app.session.get('UserFilters')|length) >0 %}
    {% set filter = app.session.get('user_filter') %}     
       {% set filters = app.session.get('UserFilters') %}     
        {% for k,filter in filters %}
            { id:"filter_{{ filter.id }}", text: "{{ filter.name }}" },
        {% endfor %}
    {% endif %}
                  ]
              },
              {id:"filter_edit","type":"button",text:"","img":"edit.png","tooltip": "{{ "Edit filter" | trans }}" },
              {id:"filter_all","type":"button",text:"","img":"zoom_out.png"},
          ]
      }
   ]
}
