{
   "items":
   [
      {   "type":"block", 
          text:"{{ "module.I5" | trans }}", 
          "text_pos": "top", 
          "list":
          [
             {   "type": "button", 
                 text:"", 
                 "isbig": true, 
                 "img": "48/i5.png"
              },
              {  "type": "buttonSelect", 
                 text:"", 
                 "img": "view.png", 
                 items:
                 [ 
                    { id:"menu_jobs", text: "{{ "Jobs" | trans }}", img: "job.png" },
                    {   type: "buttonSelect", 
                        text: "{{ "Jobs" | trans }}", 
                        img:  "job.png", 
                        items:
                        [ 
                            { id:"menu_jobs", text: "{{ "Active jobs" | trans }}" },
                            { id:"menu_submitted", text: "{{ "Submitted jobs" | trans }}" },
                            { id:"menu_scheduled", text: "{{ "Scheduled jobs" | trans }}" }                            
                        ]
                    },
                    { id:"menu_messages", text: "{{ "Messages" | trans }}", img: "message.png" }
                 ]
              },
              {     "type":"button",
                    id:"menu_console", 
                    tooltip: "{{ "Console" | trans }}", 
                    img: "console.png"
              },
              {type : "newLevel"},
{% set current_db = app.session.get('spooler') %} 
              {   id: "spooler", 
                  "type":"buttonSelect", 
                  text:"", 
                  "img": "spooler.png",
                  "items": 
                  [ 
{% for spooler in Spoolers %}
{id:"{{ spooler.id }}", text:"{{ spooler.name }}"{% if spooler.name == current_db.name %}, img: "accept.png"{% endif %} },
{% endfor %}
                  ]
               },
            {   "type": "buttonSelect", 
                 text:"",
                 "id": "Filters",
                 "img": "zoom.png", 
                 items:
                 [ 
              {id:"filter_edit","type":"button",text:"{{ "New" | trans }}","img":"zoom_in.png","tooltip": "{{ "Edit filter" | trans }}" },
     {% if (app.session.get('UserFilters')|length) >0 %}
            { id:"sepf0", type: "separator" },    
    {% set current = app.session.get('user_filter') %}     
       {% set filters = app.session.get('UserFilters') %}     
        {% for k,filter in filters %}
            { id:"filter_{{ filter.id }}", text: "{{ filter.name }}" {% if filter.name == current.name %}, img: "accept.png"{% endif %}  },
        {% endfor %}
    {% endif %}
                  ]
              },
              {id:"filter_all","type":"button",tooltip:"{{ "All" | trans }}","img":"zoom_out.png"}
              
          ]
      }  
    ]
}
